<div class="container">
<% if @projects.empty? %>
  <p>You do not have any projects...</p>
  <% else %>
  <ol class="project">
  <% @projects.each do |project| %>
    <li><a href="/projects/<%= project.id%>"><%= project.name%></a></li>
  <% end %>
  </ol>

<% end %>
</div>

<div class="container">
  <a href="/projects/new">Add new project</a>
</div>
