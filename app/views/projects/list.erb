<h1 class="text-center">List of Projects</h1>

<div class="container">
  <% @projects.each do |project| %>
  <ol class="project">
    <li><a href="/projects/<%= project.id%>"><%= project.name%></a></li>
  </ol>
  <% end %>
</div>


<div class="container">
  <a href="/projects/new">Add new project</a>
</div>
